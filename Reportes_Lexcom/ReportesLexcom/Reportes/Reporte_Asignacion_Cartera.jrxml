	<parameter name="corporacion" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
d.caso CASO,
d.fecha_recepcion FECHA_RECEPCION,
d.antiguedad ANTIGUEDAD,
(select dh1.descripcion from deudor_historial_cobros dh1 where dh1.deudor=d.deudor and (dh1.deudor,dh1.deudor_historial_cobros) in (select dh.deudor deudor, max(dh.deudor_historial_cobros) ultima_gestion from deudor_historial_cobros dh where dh.deudor=d.deudor)) ULTIMA_GESTION,
CAST(d.no_cuenta AS CHAR) NO_CREDITO,
d.nombre NOMBRE_DEUDOR,
e.nombre STATUS_JUDICIAL,
s.nombre ESTADO_JUDICIAL,
ee.nombre STATUS_EXTRAJUDICIAL,
se.nombre ESTADO_EXTRAJUDICIAL,
d.moneda MONEDA,
d.monto_inicial MONTO_INICIAL,
if(d.opcion_proximo_pago='SI', d.cuota_convenio, null) CUOTA,
(select p.fecha_registro from pago p where p.deudor=d.deudor and (p.deudor, p.fecha_registro) in (select pa.deudor, max(pa.fecha_registro) from pago pa where pa.deudor=d.deudor group by pa.deudor) group by p.fecha_registro) ULTIMO_PAGO_REGISTRO,
(select p.fecha from pago p where p.deudor=d.deudor and (p.deudor, p.fecha) in (select pa.deudor, max(pa.fecha) from pago pa where pa.deudor=d.deudor group by pa.deudor) group by p.fecha) ULTIMO_PAGO_BANCO,
(select sum(pag.monto) from pago pag where pag.deudor=d.deudor) PAGO_MES,
if(pro.estado_promesa='PENDIENTE', pro.fecha_creacion, null) FECHA_CREACION_PROX_CUOTA,
if(d.opcion_proximo_pago='SI', d.fecha_proximo_pago, null) FECHA_PROXIMO_PAGO,
g.nombre GARANTIA,
u.nombre GESTOR,
cc.codigo CODIGO_RESULTADO,
a.nombre ACTOR,
d.saldo SALDO,
d.convenio_pactado CONVENIO_PACTADO
from
deudor d
left join estatus e on (d.estatus=e.estatus)
left join sestado s on (d.sestado=s.sestado)
left join estatus_extra ee on (d.estatus_extra=ee.estatus_extra)
left join sestado_extra se on (d.sestado_extra=se.sestado_extra)
left join garantia g on (d.garantia=g.garantia)
left join usuario u on (d.usuario=u.usuario)
left join codigo_contactabilidad cc on (d.codigo_contactabilidad=cc.codigo_contactabilidad)
left join actor a on (d.actor=a.actor)
left join cooperacion cor on (a.cooperacion=cor.cooperacion)
left join intencion_pago i on (d.intencion_pago=i.intencion_pago)
left join (select c3.deudor, c3.convenio, c3.tipo_convenio, c3.estado, c3.fecha_creacion, c3.fecha_activacion, c3.fecha_terminacion, c3.total_pagar, c3.cuota_inicial, c3.numero_cuotas, c3.frecuencia, c3.monto_cuota, c3.observacion, c3.interes, c3.mora, c3.gasto_otros, c3.rebaja_interes, c3.costas, c3.monto_costas, c3.total, c3.fecha_pago_inicial, c3.saldo from convenio c3 where (c3.deudor, c3.fecha_creacion, c3.convenio) in (select c2.deudor deudor, c2.fecha_creacion fecha_creacion, max(c2.convenio) convenio from convenio c2 where (c2.deudor,c2.fecha_creacion) in (select c1.deudor, max(c1.fecha_creacion) from convenio c1 group by c1.deudor) group by c2.deudor, c2.fecha_creacion)) con on (d.deudor=con.deudor)
left join (select (select c.deudor from convenio c where c.convenio=pp4.convenio) deudor, pp4.convenio, pp4.promesa_pago, pp4.estado_promesa, pp4.fecha_pago, pp4.fecha_creacion, pp4.fecha_anulacion, pp4.fecha_cumplimiento, pp4.monto from convenio_detalle pp4 where (pp4.convenio, pp4.fecha_pago, pp4.hora_pago, pp4.promesa_pago) in (select pp3.convenio, pp3.fecha_pago, pp3.hora_pago, max(pp3.promesa_pago) numero from convenio_detalle pp3 where (pp3.convenio, pp3.fecha_pago, pp3.hora_pago) in (select pp2.convenio, pp2.fecha_pago, max(pp2.hora_pago) from convenio_detalle pp2 where (pp2.convenio, pp2.fecha_pago) in (select pp1.convenio, max(pp1.fecha_pago) from convenio_detalle pp1 group by pp1.convenio) group by pp2.convenio, pp2.fecha_pago) group by pp3.convenio, pp3.fecha_pago, pp3.hora_pago) group by pp4.convenio, pp4.fecha_pago, pp4.hora_pago, pp4.promesa_pago) pro on (con.deudor=pro.deudor and con.convenio=pro.convenio)
where
(d.cargado='CARGADO') and
(cor.nombre like '$P!{corporacion}')]]>
	</queryString>
