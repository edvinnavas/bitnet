	<queryString>
		<![CDATA[select
cor.nombre CORPORACION,
d.caso CASO,
a.nombre ACTOR,
DATE_FORMAT(d.fecha_recepcion, "%d/%m/%Y") FECHA_RECEPCION,
d.nombre NOMBRE_DEUDOR,
d.antiguedad ANTIGUEDAD,
CAST(d.no_cuenta AS CHAR) NO_CREDITO,
CAST(d.no_cuenta_otro AS CHAR) OTRO_NO_CREDITO,
u.nombre GESTOR_ASIGNADO,
d.monto_inicial MONTO_INICIAL,
d.moneda MONEDA,
d.saldo SALDO,
resumen_pagos_rpt_empresas(d.deudor) RESUMEN_PAGOS,
concat(ifnull((select con1.tipo_convenio from convenio con1 where con1.deudor=d.deudor and (con1.deudor, con1.convenio) in (select con.deudor, max(con.convenio) from convenio con where con.deudor=d.deudor group by con.deudor)),'*'), ' - ', ifnull((select con1.estado from convenio con1 where con1.deudor=d.deudor and (con1.deudor, con1.convenio) in (select con.deudor, max(con.convenio) from convenio con where con.deudor=d.deudor group by con.deudor)),'*')) CONVENIO,
ifnull(if(d.opcion_proximo_pago='SI',DATE_FORMAT(d.fecha_proximo_pago, "%d/%m/%Y"),'-'),'-') FECHA_PROXIMO_PAGO,
if(d.opcion_proximo_pago='SI',d.cuota_convenio,0.00) Q_PROXIMO_PAGO,
concat(se.nombre,' - ',ee.nombre) ESTADO_ESTATUS_EXTRAJUDICIAL,
concat('(',ifnull((select u.nombre from usuario u where u.usuario in (select dhc.usuario from deudor_historial_cobros dhc where dhc.deudor_historial_cobros=get_id_ultima_gestion_contacto(d.deudor))),'-'),')*',ifnull((select DATE_FORMAT(dhc.fecha, "%d/%m/%Y") from deudor_historial_cobros dhc where dhc.deudor_historial_cobros=get_id_ultima_gestion_contacto(d.deudor)),'-'),'--',ifnull((select dhc.descripcion from deudor_historial_cobros dhc where dhc.deudor_historial_cobros=get_id_ultima_gestion_contacto(d.deudor)),'*'),'//') ULTIMA_GESTION_DE_CONTACTO,
resumen_gestiones_rpt_empresas(d.deudor, $P{fecha1}, $P{fecha2}) RESUMEN_DE_GESTIONES,
concat(s.nombre,' - ',e.nombre) ESTADO_ESTATUS_JUDICIAL,
situacion_judicial_rpt_empresas(d.deudor, $P{fecha1}, $P{fecha2}) SITUACION_JUDICIAL,
d.descripcion ACTUALIZACION_CASO
from
deudor d
left join estatus e on (d.estatus=e.estatus)
left join sestado s on (d.sestado=s.sestado)
left join estatus_extra ee on (d.estatus_extra=ee.estatus_extra)
left join sestado_extra se on (d.sestado_extra=se.sestado_extra)
left join usuario u on (d.usuario=u.usuario)
left join actor a on (d.actor=a.actor)
left join cooperacion cor on (a.cooperacion=cor.cooperacion)
where
(cor.nombre like '$P!{corporacion}') and 
(a.nombre like '$P!{actor}') and 
(d.cargado like '$P!{cargado}')]]>
	</queryString>
