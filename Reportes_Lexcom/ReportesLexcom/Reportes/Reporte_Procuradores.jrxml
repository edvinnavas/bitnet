	<queryString>
		<![CDATA[select
d.caso Caso,
d.cargado Cargado,
a.nombre Actor,
d.no_cuenta No_cuenta,
d.no_cuenta_otro Otro_no_cuenta,
g.nombre Garantia,
d.nombre Nombre_cliente,
if(d.moneda='QUETZAL',d.monto_inicial,0.00) MontoQQ,
if(d.moneda='DOLLAR',d.monto_inicial,0.00) MontoUSD,
gs.nombre Gestor,
s.nombre Estado_judicial,
e.nombre Estatus_judicial,
sx.nombre Estado_extrajudicial,
ex.nombre Estatus_extrajudicial,
d.situacion_caso Situacion_juicio,
jg.nombre Juzgado,
j.no_juicio No_juicio,
j.notificador Notificador,
u.nombre Procurador,
j.procuracion Procuracion,
j.razon_notificacion Razon_notificacion,
j.fecha Presentacion_demanda,
j.fecha_admision_demanda Admision_demanda,
j.sumario Sumario,
j.memorial Memorial,
j.deudor_notificado Notificado,
j.fecha_notificacion Fecha_notificado,
if(d.opcion_proximo_pago='NO',date('1900-01-01'),d.fecha_proximo_pago) Fecha_proximo_pago,
if(d.opcion_proximo_pago='NO','0.00',d.cuota_convenio) Monto_proximo_pago,
(select ifnull(max(p.fecha),date('1900-01-01')) from pago p where p.deudor=d.deudor) Fecha_ultimo_pago,
d.dpi DPI,
if(d.moneda='QUETZAL',d.saldo,0.00) SaldoQQ,
if(d.moneda='DOLLAR',d.saldo,0.00) SaldoUSD,
j.monto Monto_demanda,
j.abogado_deudor Aboagdo_deudor,
d.anticipo Anticipo,
(select p.monto from pago p where p.pago=get_id_ultimo_pago(d.deudor)) Monto_ultimo_pago,
d.nit Nit,
d.pdf PDF
from
deudor d
left join juicio j on (d.deudor = j.deudor)
left join usuario u on (j.procurador = u.usuario)
left join garantia g on (d.garantia = g.garantia)
left join estatus e on (d.estatus = e.estatus)
left join sestado s on (d.sestado = s.sestado)
left join juzgado jg on (j.juzgado = jg.juzgado)
left join actor a on (d.actor = a.actor)
left join estatus_extra ex on (d.estatus_extra = ex.estatus_extra)
left join sestado_extra sx on (d.sestado_extra = sx.sestado_extra)
left join usuario gs on (d.usuario = gs.usuario)
left join cooperacion co on (a.cooperacion = co.cooperacion)
where
d.cargado like '$P!{cargado}' and
u.nombre like '$P!{procurador}' and
g.nombre like '$P!{garantia}' and
e.nombre like '$P!{status}' and
s.nombre like '$P!{estado}' and
jg.nombre like '$P!{juzgado}' and 
co.nombre like '$P!{corporacion}']]>
	</queryString>
