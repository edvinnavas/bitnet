	<queryString>
		<![CDATA[select
act.nombre actor,
deu.caso caso,
deu.fecha_recepcion fecha_recepcion,
deu.antiguedad antiguedad,
gar.nombre garantia,
usu.nombre gestor,
deu.no_cuenta no_cuenta,
deu.nombre nombre_deudor,
deu.monto_inicial monto_inicial,
deu.saldo saldo,
ifnull((select sum(p.monto) monto from pago p where p.deudor=deu.deudor group by p.deudor),0.00) pagos_acumulados,
ifnull(con.tipo_convenio,'-') tipo_convenio,
ifnull(con.estado,'-') estado_convenio,
ifnull(con.fecha_creacion,date('1900-01-01')) fecha_convenio,
ifnull(con.saldo,0.00) saldo_convenio,
ifnull(con.interes,0.00) interes,
ifnull(con.mora,0.00) mora,
ifnull(con.gasto_otros,0.00) gastos_otros,
ifnull(con.costas,0.00) porcentaje_costas,
ifnull(con.monto_costas,0.00) q_monto_costas,
ifnull(con.rebaja_interes,0.00) rebaja_interes,
ifnull(con.total,0.00) total_convenio,
ifnull(con.cuota_inicial,0.00) q_pago_inicial,
ifnull(con.total_pagar,0.00) total_pagar,
ifnull(con.numero_cuotas,0) cantidad_cuotas,
ifnull(con.frecuencia,'-') frecuencia_cuotas,
ifnull(con.monto_cuota,0.00) monto_cuota,
ifnull(con.observacion,'-') convenio_pactado,
ifnull(con.fecha_pago_inicial,date('1900-01-01')) fecha_pago_inicial,
if(ifnull(con.estado,'-')='ANULADO','INCUMPLIDO',if(ifnull(con.estado,'-')='NEGOCIACION','INCUMPLIDO',ifnull(pro.estado_promesa,'-'))) estado_pp,
if(ifnull(con.estado,'-')='ANULADO',date('1900-01-01'),if(ifnull(con.estado,'-')='NEGOCIACION',date('1900-01-01'),deu.fecha_proximo_pago)) f_prox_cuota,
if(ifnull(con.estado,'-')='ANULADO',0.00,if(ifnull(con.estado,'-')='NEGOCIACION',0.00,deu.cuota_convenio)) q_prox_cuota,
(select count(*) from convenio_detalle cd where cd.estado_promesa='CUMPLIDO' and cd.convenio=con.convenio) promesas_cumplidas,
(select count(*) from convenio_detalle cd where cd.estado_promesa='INCUMPLIDO' and cd.convenio=con.convenio) promesas_incumplidas,
(select ifnull(max(p.fecha),date('1900-01-01')) from pago p where p.deudor=con.deudor and p.fecha>=con.fecha_creacion) ultimo_pago,
(select ifnull(count(*),0) from pago p where p.deudor=con.deudor and p.fecha>=con.fecha_creacion) cant_pagos_convenio,
(select ifnull(sum(p.monto),0) from pago p where p.deudor=con.deudor and p.fecha>=con.fecha_creacion) q_pagos_convenio,
ifnull(con.numero_cuotas,0) - (select ifnull(count(*),0) from pago p where p.deudor=con.deudor and p.fecha>=con.fecha_creacion) cuotas_pendientes,
se.nombre estado_judicial,
es.nombre estatus_judicial,
see.nombre estado_extrajudicial,
ese.nombre estatus_extrajudicial,
ifnull((select dhc.contacto from deudor_historial_cobros dhc where dhc.deudor_historial_cobros=get_id_ultima_gestion(deu.deudor)),'-') contacto,
ifnull((select j.no_juicio from juicio j where j.deudor=deu.deudor),'-') no_juicio,
trim(concat(deu.telefono_casa,' ',deu.telefono_celular,' ',deu.telefono_trabajo)) telefono,
ifnull((select p.fecha from pago p where p.pago=get_id_ultimo_pago(deu.deudor)),date('1900-01-01')) fecha_ultimo_pagpo,
ifnull((select p.monto from pago p where p.pago=get_id_ultimo_pago(deu.deudor)),0.00) monto_ultimo_pagpo
from
deudor deu
left join garantia gar on (deu.garantia=gar.garantia)
left join usuario usu on (deu.usuario=usu.usuario)
left join actor act on (deu.actor=act.actor)
left join cooperacion cor on (act.cooperacion=cor.cooperacion)
left join sestado se on (deu.sestado=se.sestado)
left join estatus es on (deu.estatus=es.estatus)
left join sestado_extra see on (deu.sestado_extra=see.sestado_extra)
left join estatus_extra ese on (deu.estatus_extra=ese.estatus_extra)
left join (select c3.deudor, c3.convenio, c3.tipo_convenio, c3.estado, c3.fecha_creacion, c3.total_pagar, c3.cuota_inicial, c3.numero_cuotas, c3.frecuencia, c3.monto_cuota, c3.observacion, c3.interes, c3.mora, c3.gasto_otros, c3.rebaja_interes, c3.costas, c3.monto_costas, c3.total, c3.fecha_pago_inicial, c3.saldo from convenio c3 where (c3.deudor, c3.fecha_creacion, c3.convenio) in (select c2.deudor deudor, c2.fecha_creacion fecha_creacion, max(c2.convenio) convenio from convenio c2 where (c2.deudor,c2.fecha_creacion) in (select c1.deudor, max(c1.fecha_creacion) from convenio c1 group by c1.deudor) group by c2.deudor, c2.fecha_creacion)) con on (deu.deudor=con.deudor)
left join (select (select c.deudor from convenio c where c.convenio=pp4.convenio) deudor, pp4.convenio, pp4.promesa_pago, pp4.estado_promesa from convenio_detalle pp4 where (pp4.convenio, pp4.fecha_pago, pp4.hora_pago, pp4.promesa_pago) in (select pp3.convenio, pp3.fecha_pago, pp3.hora_pago, max(pp3.promesa_pago) numero from convenio_detalle pp3 where (pp3.convenio, pp3.fecha_pago, pp3.hora_pago) in (select pp2.convenio, pp2.fecha_pago, max(pp2.hora_pago) from convenio_detalle pp2 where (pp2.convenio, pp2.fecha_pago) in (select pp1.convenio, max(pp1.fecha_pago) from convenio_detalle pp1 group by pp1.convenio) group by pp2.convenio, pp2.fecha_pago) group by pp3.convenio, pp3.fecha_pago, pp3.hora_pago) group by pp4.convenio, pp4.fecha_pago, pp4.hora_pago, pp4.promesa_pago) pro on (con.deudor=pro.deudor and con.convenio=pro.convenio)
where
cor.nombre like '$P!{corporacion}' and
cor.nombre like '$P!{actor}' and
deu.cargado='$P!{cargado}']]>
