	<parameter name="corporacion" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
a.nombre ACTOR,
d.caso CASO,
d.antiguedad ANTIGUEDAD,
g.nombre GARANTIA,
d.fecha_recepcion FECHA_RECEPCION,
CAST(d.no_cuenta AS CHAR) NO_CREDITO,
d.nombre NOMBRE_DEUDOR,
u.nombre GESTOR_ASIGNADO,
s.nombre ESTADO_JUDICIAL,
e.nombre STATUS_JUDICIAL,
d.monto_inicial MONTO_INICIAL,
d.moneda MONEDA,
d.saldo SALDO,
ifnull((select sum(pag.monto) from pago pag where pag.deudor=d.deudor),0.00) TOTAL_PAGOS,
ifnull((select p.fecha_registro from pago p where p.deudor=d.deudor and (p.deudor, p.fecha_registro) in (select pa.deudor, max(pa.fecha_registro) from pago pa where pa.deudor=d.deudor group by pa.deudor) group by p.fecha_registro),date('1900-01-01')) ULTIMO_PAGO_REGISTRO,
ifnull((select p.fecha from pago p where p.deudor=d.deudor and (p.deudor, p.fecha) in (select pa.deudor, max(pa.fecha) from pago pa where pa.deudor=d.deudor group by pa.deudor) group by p.fecha),date('1900-01-01')) ULTIMO_PAGO_BANCO,
ifnull((select p.monto from pago p where p.deudor=d.deudor and (p.deudor, p.fecha) in (select pa.deudor, max(pa.fecha) from pago pa where pa.deudor=d.deudor group by pa.deudor) group by p.fecha),0.00) Q_ULTIMO_PAGO,
ifnull((select con1.estado from convenio con1 where con1.deudor=d.deudor and (con1.deudor, con1.convenio) in (select con.deudor, max(con.convenio) from convenio con where con.deudor=d.deudor group by con.deudor)),'-') ESTADO_CONVENIO,
ifnull((select con1.tipo_convenio from convenio con1 where con1.deudor=d.deudor and (con1.deudor, con1.convenio) in (select con.deudor, max(con.convenio) from convenio con where con.deudor=d.deudor group by con.deudor)),'-') TIPO_CONVENIO,
ifnull(if(d.opcion_proximo_pago='SI',d.fecha_proximo_pago,date('1900-01-01')),date('1900-01-01')) FECHA_PROXIMO_PAGO,
if(d.opcion_proximo_pago='SI',d.cuota_convenio,0.00) Q_PROXIMO_PAGO,
se.nombre ESTADO_EXTRAJUDICIAL,
ee.nombre STATUS_EXTRAJUDICIAL,
i.nombre INTENSION_PAGO,
ifnull((select max(dh.fecha) from deudor_historial_cobros dh where dh.codigo_contactabilidad in (2,3,11,12) and dh.deudor=d.deudor),date('1900-01-01')) FECHA_ULTIMO_CONTACTO,
ifnull((select u.nombre from deudor_historial_cobros dh1 left join usuario u on (dh1.usuario=u.usuario) where dh1.deudor=d.deudor and (dh1.deudor,dh1.deudor_historial_cobros) in (select dh.deudor deudor, max(dh.deudor_historial_cobros) ultima_gestion from deudor_historial_cobros dh where dh.deudor=d.deudor group by dh.deudor)),'-') COD_USUARIO_ULTIMA_GESTION,
ifnull((select max(dh.fecha) ultima_gestion from deudor_historial_cobros dh where dh.deudor=d.deudor),date('1900-01-01')) F_ULTIMA_GESTION,
cc.codigo CODIGO_RESULTADO,
ifnull((select dh1.descripcion from deudor_historial_cobros dh1 where dh1.deudor=d.deudor and (dh1.deudor,dh1.deudor_historial_cobros) in (select dh.deudor deudor, max(dh.deudor_historial_cobros) ultima_gestion from deudor_historial_cobros dh where dh.deudor=d.deudor group by dh.deudor)),'-') TEXTO_ULTIMA_GESTION,
(select j.no_juicio from juicio j where j.deudor=d.deudor) NO_JUICIO,
d.situacion_caso SITUACION_JUDICIAL,
ifnull((select jf1.deligenciado from juicio_finca jf1 left join juicio j on (jf1.juicio=j.juicio) where j.deudor=d.deudor and (jf1.juicio,jf1.deligenciado) in (select jf.juicio, max(jf.deligenciado) fecha_deligenciado from juicio_finca jf group by jf.juicio) group by j.juicio),date('1900-01-01')) FECHA_NOTIFICACION_FINCA,
ifnull((select concat('FINCA: ',jf1.finca,' LETRA: ',jf1.letra) from juicio_finca jf1 left join juicio j on (jf1.juicio=j.juicio) where j.deudor=d.deudor and (jf1.juicio,jf1.deligenciado) in (select jf.juicio, max(jf.deligenciado) fecha_deligenciado from juicio_finca jf group by jf.juicio) group by j.juicio),'-') FINCA
from
deudor d
left join estatus e on (d.estatus=e.estatus)
left join sestado s on (d.sestado=s.sestado)
left join estatus_extra ee on (d.estatus_extra=ee.estatus_extra)
left join sestado_extra se on (d.sestado_extra=se.sestado_extra)
left join garantia g on (d.garantia=g.garantia)
left join usuario u on (d.usuario=u.usuario)
left join codigo_contactabilidad cc on (d.codigo_contactabilidad=cc.codigo_contactabilidad)
left join actor a on (d.actor=a.actor)
left join cooperacion cor on (a.cooperacion=cor.cooperacion)
left join intencion_pago i on (d.intencion_pago=i.intencion_pago)
where
(d.cargado='CARGADO') and
(cor.nombre like '$P!{corporacion}')]]>
