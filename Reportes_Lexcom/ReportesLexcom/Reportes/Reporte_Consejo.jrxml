		<defaultValueExpression><![CDATA[($P{fecha}==null?"01/01/13":(new SimpleDateFormat("dd/MM/yy")).format(( new SimpleDateFormat("EEE MMM dd HH:mm:ss ZZZ yyyy", Locale.ENGLISH)).parse($P{fecha}.toString())))]]></defaultValueExpression>
	</parameter>
	<parameter name="fechafinc" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{fechafin}==null?"01/01/15":(new SimpleDateFormat("dd/MM/yy")).format(( new SimpleDateFormat("EEE MMM dd HH:mm:ss ZZZ yyyy", Locale.ENGLISH)).parse($P{fechafin}.toString())))]]></defaultValueExpression>
	</parameter>
	<parameter name="fechac_1" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{fecha_1}==null?"01/01/13":(new SimpleDateFormat("dd/MM/yy")).format(( new SimpleDateFormat("EEE MMM dd HH:mm:ss ZZZ yyyy", Locale.ENGLISH)).parse($P{fecha_1}.toString())))]]></defaultValueExpression>
	</parameter>
	<parameter name="fechafinc_1" class="java.lang.String">
		<defaultValueExpression><![CDATA[($P{fechafin_1}==null?"01/01/15":(new SimpleDateFormat("dd/MM/yy")).format(( new SimpleDateFormat("EEE MMM dd HH:mm:ss ZZZ yyyy", Locale.ENGLISH)).parse($P{fechafin_1}.toString())))]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_1" class="java.util.Date">
