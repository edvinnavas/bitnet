	<queryString>
		<![CDATA[SELECT
d.caso CASO,
u.nombre GESTOR,
d.no_cuenta_otro OTRO_NO_CUENTA,
d.cargado CARGADO,
a.nombre ACTOR,
s.nombre ESTADO_JURIDICO,
e.nombre ESTATUS_JURIDICO,
d.fecha_recepcion FECHA_RECEPCION,
j.fecha PRESENTACION_DEMANDA,
j.tiempo_estimado TIEMPO_ESTIMADO,
ju.nombre JUZGADO,
j.no_juicio NUMERO_JUICIO,
d.no_cuenta NUMERO_CREDITO,
d.nombre NOMBRE_DEUDOR,
d.moneda TIPO_MONEDA,
d.monto_inicial MONTO,
d.saldo SALDO,
g.nombre GARANTIA,
d.convenio_pactado CONVENIO,
CONCAT(IFNULL(M_ANT.ANTERIORES_ACCIONES,'-'),IFNULL(J_ANT.ANTERIORES_ACCIONES,'-'),IFNULL(P_ANT.ANTERIORES_ACCIONES,'-'),IFNULL(JU_ANT.ANTERIORES_ACCIONES,'-')) ANTERIORES_ACCIONES,
CONCAT(IFNULL(M_SIG.ULTIMAS_ACCIONES,'-'),IFNULL(J_SIG.ULTIMAS_ACCIONES,'-'),IFNULL(P_SIG.ULTIMAS_ACCIONES,'-'),IFNULL(JU_SIG.ULTIMAS_ACCIONES,'-')) ULTIMAS_ACCIONES,
d.situacion_caso SITUACION_CASO
FROM
deudor d
LEFT JOIN actor a ON (d.actor=a.actor)
LEFT JOIN cooperacion cor ON (a.cooperacion=cor.cooperacion)
LEFT JOIN sestado s ON (d.sestado=s.sestado)
LEFT JOIN estatus e ON (d.estatus=e.estatus)
LEFT JOIN juicio j ON (d.deudor=j.deudor)
LEFT JOIN juzgado ju ON (j.juzgado=ju.juzgado)
LEFT JOIN garantia g ON (d.garantia=g.garantia)
LEFT JOIN usuario u on (d.usuario=u.usuario)
LEFT JOIN (SELECT
dhc.deudor COD_DEUDOR,
GROUP_CONCAT('- Historial cobros: ',dhc.descripcion,'\n') ANTERIORES_ACCIONES
FROM
deudor_historial_cobros dhc
WHERE
(dhc.fecha >= $P{fecha_anteriores_acciones_inicial} and dhc.fecha <= $P{fecha_anteriores_acciones_final})
GROUP BY
dhc.deudor
ORDER BY
dhc.fecha) M_ANT ON (d.deudor=M_ANT.COD_DEUDOR)
LEFT JOIN (SELECT
dhc.deudor COD_DEUDOR,
GROUP_CONCAT('- Historial cobros: ',dhc.descripcion,'\n') ULTIMAS_ACCIONES
FROM
deudor_historial_cobros dhc
WHERE
(dhc.fecha >= $P{fecha_ultimas_acciones_inicial} and dhc.fecha <= $P{fecha_ultimas_acciones_final})
GROUP BY
dhc.deudor
ORDER BY
dhc.fecha) M_SIG ON (d.deudor=M_SIG.COD_DEUDOR)
LEFT JOIN (SELECT
dhj.deudor COD_DEUDOR,
GROUP_CONCAT('- Historial jurídico: ',dhj.descripcion,'\n') ANTERIORES_ACCIONES
FROM
deudor_historial_juridico dhj
WHERE
(dhj.fecha >= $P{fecha_anteriores_acciones_inicial} and dhj.fecha <= $P{fecha_anteriores_acciones_final})
GROUP BY
dhj.deudor
ORDER BY
dhj.fecha) J_ANT ON (d.deudor=J_ANT.COD_DEUDOR)
LEFT JOIN (SELECT
dhj.deudor COD_DEUDOR,
GROUP_CONCAT('- Historial jurídico: ',dhj.descripcion,'\n') ULTIMAS_ACCIONES
FROM
deudor_historial_juridico dhj
WHERE
(dhj.fecha >= $P{fecha_ultimas_acciones_inicial} and dhj.fecha <= $P{fecha_ultimas_acciones_final})
GROUP BY
dhj.deudor
ORDER BY
dhj.fecha) J_SIG ON (d.deudor=J_SIG.COD_DEUDOR)
LEFT JOIN (SELECT
p.deudor COD_DEUDOR,
GROUP_CONCAT('- Pago realizado ', p.fecha_registro, ' Q.', p.monto, '\n') ANTERIORES_ACCIONES
FROM
pago p
WHERE
(p.fecha_registro >= $P{fecha_anteriores_acciones_inicial} and p.fecha_registro <= $P{fecha_anteriores_acciones_final})
GROUP BY
p.deudor
ORDER BY
p.fecha_registro) P_ANT ON (d.deudor=P_ANT.COD_DEUDOR)
LEFT JOIN (SELECT
p.deudor COD_DEUDOR,
GROUP_CONCAT('- Pago realizado ', p.fecha_registro, ' Q.', p.monto, '\n') ULTIMAS_ACCIONES
FROM
pago p
WHERE
(p.fecha_registro >= $P{fecha_ultimas_acciones_inicial} and p.fecha_registro <= $P{fecha_ultimas_acciones_final})
GROUP BY
p.deudor
ORDER BY
p.fecha_registro) P_SIG ON (d.deudor=P_SIG.COD_DEUDOR)
LEFT JOIN (SELECT
ju.deudor COD_DEUDOR,
GROUP_CONCAT('- Presentación Demanda ', ju.fecha, ' Numero Juicio: ', ju.no_juicio, ' ',ju.procuracion, '\n') ANTERIORES_ACCIONES
FROM
juicio ju
WHERE
(ju.fecha >= $P{fecha_anteriores_acciones_inicial} and ju.fecha <= $P{fecha_anteriores_acciones_final})
GROUP BY
ju.deudor
ORDER BY
ju.fecha) JU_ANT ON (d.deudor=JU_ANT.COD_DEUDOR)
LEFT JOIN (SELECT
ju.deudor COD_DEUDOR,
GROUP_CONCAT('- Presentación Demanda ', ju.fecha, ' Numero Juicio: ', ju.no_juicio, ' ',ju.procuracion, '\n') ULTIMAS_ACCIONES
FROM
juicio ju
WHERE
(ju.fecha >= $P{fecha_ultimas_acciones_inicial} and ju.fecha <= $P{fecha_ultimas_acciones_final})
GROUP BY
ju.deudor
ORDER BY
ju.fecha) JU_SIG ON (d.deudor=JU_SIG.COD_DEUDOR)
WHERE
cor.nombre like '%$P!{corporacion}%']]>
	</queryString>
