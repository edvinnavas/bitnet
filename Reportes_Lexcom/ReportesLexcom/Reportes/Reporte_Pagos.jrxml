	<queryString>
		<![CDATA[select
c.nombre Corporacion,
d.caso Caso,
a.nombre Actor,
d.fecha_recepcion Fecha_recepcion,
d.moneda Moneda,
d.nombre Nombre_deudor,
g.nombre Garantia,
d.antiguedad Antiguedad,
s.nombre Estado_Judicial,
e.nombre Estatus_Judicial,
ee.nombre Estado_Extrajudicial,
se.nombre Estatus_Extrajudicial,
u.nombre Gestor,
d.no_cuenta No_credito,
d.no_cuenta_otro Otro_no_credito,
d.monto_inicial Monto_inicial,
d.saldo Saldo_al_dia,
cc.nombre Codigo_resultado,
p.monto Pago_mes,
p.fecha Fecha_boleta,
b.nombre Banco,
p.fecha_registro Fecha_pago_registro,
p.no_boleta No_boleta,
if(d.opcion_proximo_pago='SI',d.fecha_proximo_pago,null) Fecha_proximo_pago,
if(d.opcion_proximo_pago='SI', d.cuota_convenio, 0.00) Cuota,
(select dhc.fecha from deudor_historial_cobros dhc where dhc.deudor_historial_cobros=get_id_ultima_gestion(d.deudor)) Ultima_gestion
from
deudor d
left join estatus e on (d.estatus=e.estatus)
left join sestado s on (d.sestado=s.sestado)
left join estatus_extra ee on (d.estatus_extra=ee.estatus_extra)
left join sestado_extra se on (d.sestado_extra=se.sestado_extra)
left join garantia g on (d.garantia=g.garantia)
left join usuario u on (d.usuario=u.usuario)
left join pago p on (d.deudor=p.deudor)
left join banco b on (p.banco=b.banco)
left join codigo_contactabilidad cc on (d.codigo_contactabilidad=cc.codigo_contactabilidad)
left join actor a on (d.actor=a.actor)
left join cooperacion c on (a.cooperacion=c.cooperacion)
where
(c.nombre like '$P!{corporacion}') AND
(a.nombre like '$P!{actor}') AND
(d.cargado like '$P!{cargado}') AND
(p.fecha_registro between '$P!{fecha1}' and '$P!{fecha2}') and
(p.monto > $P!{monto})]]>
	</queryString>
